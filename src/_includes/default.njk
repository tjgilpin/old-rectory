        </svg>
        <p>{{ footerlinks.address | safe }}</p>
      </div>
      <ul class="secondary">
        {% for item in footerlinks.secondary %}
          <li>
            <a href="{{ item.url }}">{{ item.title | safe }}</a>
          </li>
        {% endfor %}  
      </ul>           
      <ul class="socials">
        {% for item in footerlinks.socials %}
          <li>
            <a href="{{ item.url }}">{{ item.icon | safe }}</a>
          </li>
        {% endfor %}
      </ul>
      <div class="newsletter">
        {{ footerlinks.newsletter | safe }}
      </div>
      <div class="certification">
        <img src="{{ footerlinks.certimage }}" alt="Certification" />
        <p>
          {{ footerlinks.certtitle | safe }}
        </p>
      </div>
      <div class="copyright">
        <p>
          {{ footerlinks.copyright | safe }} Site by <a href="https://timgilp.in">Tim Gilpin</a>
        </p>
      </div>
    </footer>
  </body>
</html>
